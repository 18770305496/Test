冒泡排序:
    int[] arr = {45, 96, 31, 764, 10, 37};
    第一趟:
        第一次: 45与96比:    {45, 96, 31, 764, 10, 37};
        第二次: 96与31比:    {45, 31, 96, 764, 10, 37};
        第三次: 96与764比:   {45, 31, 96, 764, 10, 37};
        第四次: 764与10比:   {45, 31, 96, 10, 764, 37};
        第五次: 764与37比:   {45, 31, 96, 10, 37, 764};
    第一趟结束:  {45, 31, 96, 10, 37, 764};
    第二趟:
        第一次: 45与31比:    {31, 45, 96, 10, 37, 764};
        第二次: 45与96比:    {31, 45, 96, 10, 37, 764};
        第三次: 96与10比:    {31, 45, 10, 96, 37, 764};
        第四次: 96与37比:    {31, 45, 10, 37, 96, 764};
    第二趟结束:    {31, 45, 10, 37, 96, 764};
    第三趟:
        第一次: 31与45比:    {31, 45, 10, 37, 96, 764};
        第二次: 45与10比:    {31, 10, 45, 37, 96, 764};
        第三次: 45与37比:    {31, 10, 37, 45, 96, 764};
    第三趟结束:  {31, 10, 37, 45, 96, 764};
    第四趟:
        第一次: 31与10比:    {10, 31, 37, 45, 96, 764};
        第二次: 31与37比:    {10, 31, 37, 45, 96, 764};
    第四趟结束:  {10, 31, 37, 45, 96, 764};
    第五趟:
        第一次: 10与31比:    {10, 31, 37, 45, 96, 764};
    第五趟结束:  {10, 31, 37, 45, 96, 764};

    排序完成: {10, 31, 37, 45, 96, 764};






